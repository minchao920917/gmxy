/**app.wxss**/

.main {
  font-size: 0.8rem;
  background: #efeef3;
  padding-bottom: 50rpx;
}

/* footer模块样式 */

.footer {
  position: fixed;
  bottom: 0;
  height: 127rpx;
  width: 100%;
  background: #efeef3;
  display: block;

}

.foot-navs {
  box-shadow: 0 -1px 2px 0 rgba(0,0,0,.2);
  margin-top: 29rpx;
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  background: #fff;
  align-items: center;
  height: 98rpx;
  padding: 5rpx 0;
}

.foot-navs .list {
  flex-grow: 2;
  /* width: 110rpx; */
  background: #fff;
  box-sizing: border-box;
  text-align: center;
  align-content: space-around;
}

.foot-navs .list navigator {
  padding: 0;
}

.foot-navs image {
  width: 40rpx;
  height: 40rpx;
}

.foot-navs .list text {
  display: block;
  font-size: 28rpx;
  color: #535353;
}

.foot-navs .list text.active {
  color: #d1141b;
}

.zg {
  position: absolute;
  top: 0;
  left: 50%;
  width: 106rpx;
  height: 130rpx;
  margin-left: -53rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}

.zg image {
  width: 104rpx;
  height: 104rpx;
  margin-top: 15rpx;
}

/* footer模块样式 end */

.tel {
  display: flex;
  justify-content: center;
  padding: 30rpx 0;
  line-height: 48rpx;
  font-size: 0.9rem;
}

.tel image {
  width: 34rpx;
  height: 34rpx;
  vertical-align: middle;
  margin: 8rpx 10rpx 0 0;
}

/* 新闻模板 */

.market-entry scroll-view {
  width: 100%;
  height: 100vh;
}

.market-entry .enter-item {
  border-bottom: 1px solid #ddd;
  padding: 20rpx;
  line-height: 40rpx;
}

.market-entry .conten {
  display: flex;
  justify-content: space-between;
}

.market-entry .conten image {
  flex-shrink: 0;
  height: 150rpx;
  width: 120px;
  margin-left: 20rpx;
  display: inline-flex;
}

.market-entry .conten .tit {
  font-weight: bold;
  overflow: hidden;
  height: 40rpx;
  text-overflow: ellipsis;
  margin-bottom: 15rpx;
  font-size: 1rem;
}

.market-entry .conten  text {
  display: block;
}

.market-entry .conten  .infos {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #666;
}

.market-entry .time {
  display: flex;
  justify-content: space-between;
  padding-top: 16rpx;
  color: #999;
}

.market-entry .time .sours text {
  padding-right: 20rpx;
}

.market-entry .time .sours image {
  width: 36rpx;
  height: 26rpx;
  vertical-align: middle;
  margin-right: 10rpx;
  margin-top: -4rpx;
}

/* 没有内容时候的提示 */

.tip-none {
  display: flex;
  justify-content: center;
  padding-top: 20vh;
  font-size: 24px;
  color: #999;
}
