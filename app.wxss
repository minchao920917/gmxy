/* 没有内容时候的提示 */

.tip-none {
  display: flex;
  justify-content: center;
  padding-top: 20vh;
  font-size: 24px;
  color: #999;
}



.icon-lock {
  background: url(data:image/png;base64,);
}

.icon-unlock {
  background: url(data:image/png;base64,);
}
.icon {
  width: 26rpx;
  height: 26rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
  margin-right: 10rpx;
}