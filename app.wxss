/**app.wxss**/

.main {
  font-size: 0.8rem;
  background: #efeef3;
  padding-bottom: 50rpx;
}

/* footer模块样式 */

.footer {
  position: fixed;
  bottom: 0;
  height: 127rpx;
  width: 100%;
  background: #efeef3;
  display: block;
}

.foot-navs {
  box-shadow: 0 -1px 2px 0 rgba(0, 0, 0, 0.2);
  margin-top: 29rpx;
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  background: #fff;
  align-items: center;
  height: 98rpx;
  padding: 5rpx 0;
}

.foot-navs .list {
  flex-grow: 2;
  /* width: 110rpx; */
  background: #fff;
  box-sizing: border-box;
  text-align: center;
  align-content: space-around;
}

.foot-navs .list navigator {
  padding: 0;
}

.foot-navs image {
  width: 40rpx;
  height: 40rpx;
}

.foot-navs .list text {
  display: block;
  font-size: 28rpx;
  color: #535353;
}

.foot-navs .list text.active {
  color: #d1141b;
}

.zg {
  position: absolute;
  top: 0;
  left: 50%;
  width: 106rpx;
  height: 130rpx;
  margin-left: -53rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}

.zg image {
  width: 104rpx;
  height: 104rpx;
  margin-top: 15rpx;
}

/* footer模块样式 end */
