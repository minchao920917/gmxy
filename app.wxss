}
.icon-phone1 {
  background-image: url(data:image/svg+xml;base64,);
}
.icon-message1 {
  background-image: url(data:image/svg+xml;base64,);
}

.icon {
  width: 26rpx;
  height: 26rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
  margin-right: 10rpx;
}

