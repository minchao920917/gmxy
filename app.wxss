/**app.wxss**/

.main {
  font-size: 0.8rem;
  background: #efeef3;
  height: 100vh;
  max-width: 750rpx;
}

/* footer模块样式 */
.footer {
  position: fixed;
  bottom: 0;
  height: 127rpx;
  width: 100%;
  background: #efeef3;
  display: block;
  z-index: 2000;
}

.foot-navs {
  box-shadow: 0 -1px 2px 0 rgba(0, 0, 0, 0.2);
  margin-top: 29rpx;
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  background: #fff;
  align-items: center;
  /* height: 98rpx; */
  padding: 15rpx 0;
}

.foot-navs .list {
  flex-grow: 2;
    height: 98rpx;
  background: #fff;
  box-sizing: border-box;
  text-align: center;
  align-content: space-around;
}

.foot-navs .list navigator {
  padding: 0;
}

.foot-navs image {
  width: 40rpx;
  height: 40rpx;
}

.foot-navs .list text {
  display: block;
  font-size: 22rpx;
  color: #535353;
}

.foot-navs .list text.active {
  color: #d1141b;
}

.zg {
  position: absolute;
  top: 0;
  left: 50%;
  width: 106rpx;
  height: 130rpx;
  margin-left: -53rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}

.zg image {
  width: 104rpx;
  height: 104rpx;
  margin-top: 15rpx;
}

/* footer模块样式 end */
